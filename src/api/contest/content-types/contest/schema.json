{
  "kind": "collectionType",
  "collectionName": "contests",
  "info": {
    "singularName": "contest",
    "pluralName": "contests",
    "displayName": "Contest"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "",
      "required": true
    },
    "stores": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::store.store"
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "heroSubtitle": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "officialStartDate": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "officialEndDate": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "activeStores": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::store.store",
      "mappedBy": "active_contest"
    },
    "openStatus": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "enum": [
        "upcoming",
        "active",
        "ended"
      ]
    },
    "instant_prizes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::instant-prize.instant-prize",
      "mappedBy": "contest"
    },
    "sponsors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sponsor.sponsor",
      "mappedBy": "contest"
    }
  }
}
